<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/new_single_id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.findbar.SingleOrNot"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">


    <CheckBox
        android:id="@+id/pintPrice"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:checked="false"
        android:text="Low Price for Beer Pint"
        app:layout_constraintLeft_toLeftOf="@+id/Similarity"
        app:layout_constraintRight_toRightOf="@+id/Similarity"
        app:layout_constraintTop_toBottomOf="@+id/Similarity"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <CheckBox
        android:id="@+id/SingleGirls"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:checked="false"
        android:text="Places with most Single Girls"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintLeft_toLeftOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <CheckBox
        android:id="@+id/SimilarToMe"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:checked="false"
        android:text="Places similar to me"
        app:layout_constraintLeft_toLeftOf="@+id/pintPrice"
        app:layout_constraintRight_toRightOf="@+id/pintPrice"
        app:layout_constraintTop_toBottomOf="@+id/pintPrice"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <!--android:onClick="insertUserDetails"-->
    <Button
        android:id="@+id/Done"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"

        android:stateListAnimator="@null"
        android:text="Done"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <SeekBar
        android:id="@+id/MoreGirlsMoreBoys"

        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="28dp"
        android:text="More Boys"
        app:layout_constraintLeft_toLeftOf="@+id/MoreGirlsMoreBoys"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteY="28dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="2dp"
        android:text="More Girls"
        app:layout_constraintBottom_toBottomOf="@+id/SingleGirls"
        app:layout_constraintRight_toRightOf="@+id/MoreGirlsMoreBoys"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.666"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <SeekBar
        android:id="@+id/Similarity"
        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_marginTop="49dp"
        app:layout_constraintLeft_toLeftOf="@+id/Crowded"
        app:layout_constraintRight_toRightOf="@+id/Crowded"
        app:layout_constraintTop_toBottomOf="@+id/Crowded"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Less Crowded"
        app:layout_constraintBottom_toBottomOf="@+id/Crowded"
        app:layout_constraintRight_toRightOf="@+id/Crowded"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="More Crowded"
        app:layout_constraintBottom_toBottomOf="@+id/Crowded"
        app:layout_constraintLeft_toLeftOf="@+id/Crowded"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <SeekBar
        android:id="@+id/Crowded"
        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_marginTop="35dp"
        app:layout_constraintLeft_toLeftOf="@+id/MoreGirlsMoreBoys"
        app:layout_constraintRight_toRightOf="@+id/MoreGirlsMoreBoys"
        app:layout_constraintTop_toBottomOf="@+id/MoreGirlsMoreBoys"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="0dp"
        android:text="Different than Me"
        app:layout_constraintBottom_toTopOf="@+id/Similarity"
        app:layout_constraintRight_toRightOf="@+id/Similarity"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="Similar to Me"
        app:layout_constraintBottom_toBottomOf="@+id/Similarity"
        app:layout_constraintLeft_toLeftOf="@+id/Similarity"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <ToggleButton
        android:id="@+id/SinglesButton"
        style="@style/Base.TextAppearance.Widget.AppCompat.Toolbar.Subtitle"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginBottom="77dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="66dp"
        android:stateListAnimator="@null"
        android:text="ToggleButton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Done"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/SimilarToMe"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/Singles"
        android:layout_width="183dp"
        android:layout_height="57dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:text="Singles Search"
        android:textSize="25dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/SinglesButton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView8"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="334dp" />

</android.support.constraint.ConstraintLayout>
